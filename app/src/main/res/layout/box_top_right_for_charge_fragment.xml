<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_person_profile_border">

    <TextView
        android:id="@+id/txt_chargeF_national_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_top_for_layout_right_top_in_chargeF"
        android:fontFamily="@font/iran_sans_mobile"
        android:gravity="center"
        android:text="@string/txt_nationId_for_chargeF"
        android:textSize="@dimen/_5sdp"
        app:layout_constraintEnd_toStartOf="@+id/txt_chargeF_getPhoneNumber"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="0.25"
        app:layout_constraintStart_toEndOf="@+id/txt_chargeF_get_national_id"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_chargeF_get_national_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_top_for_layout_right_top_in_chargeF"
        android:fontFamily="@font/iran_sans_mobile"
        android:gravity="center"
        android:text="0253565185415"
        android:textColor="@color/black"
        android:textSize="@dimen/_5sdp"
        app:layout_constraintEnd_toStartOf="@+id/txt_chargeF_national_id"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="0.5"
        app:layout_constraintStart_toEndOf="@+id/button"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_chargeF_age"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_top_for_layout_right_top_in_chargeF"
        android:fontFamily="@font/iran_sans_mobile"
        android:gravity="center"
        android:text="@string/txt_age_for_chargeF"
        android:textSize="@dimen/_5sdp"
        app:layout_constraintEnd_toStartOf="@+id/txt_chargeF_getName"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="0.25"
        app:layout_constraintStart_toEndOf="@+id/txt_chargeF_getAge"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_chargeF_getAge"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_15sdp"
        android:fontFamily="@font/iran_sans_mobile"
        android:gravity="center"
        android:text="9سال"
        android:textColor="@color/black"
        android:textSize="@dimen/_5sdp"
        app:layout_constraintEnd_toStartOf="@+id/txt_chargeF_age"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="0.25"
        app:layout_constraintStart_toEndOf="@+id/txt_chargeF_phoneNumber"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_chargF_Name"
        android:layout_width="0dp"
        android:layout_marginEnd="@dimen/_4sdp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_top_for_layout_right_top_in_chargeF"
        android:fontFamily="@font/iran_sans_mobile"
        android:gravity="center"
        android:text="@string/txt_name_for_chargeF"
        android:textSize="@dimen/_5sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="0.25"
        app:layout_constraintStart_toEndOf="@+id/txt_chargeF_getName"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_chargeF_getName"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_top_for_layout_right_top_in_chargeF"
        android:fontFamily="@font/iran_sans_mobile"
        android:gravity="start"
        android:text="@string/txt_sample_name_for_chargF"
        android:textColor="@color/black"
        android:textSize="@dimen/_5sdp"
        app:layout_constraintBottom_toBottomOf="@+id/txt_chargF_Name"
        app:layout_constraintEnd_toStartOf="@+id/txt_chargF_Name"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="0.75"
        app:layout_constraintStart_toEndOf="@+id/txt_chargeF_age"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txt_chargeF_phoneNumber"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_top_for_layout_right_top_in_chargeF"
        android:fontFamily="@font/iran_sans_mobile"
        android:gravity="center"
        android:text=" شماره همراه :"
        android:textSize="@dimen/_5sdp"
        app:layout_constraintEnd_toStartOf="@+id/txt_chargeF_getAge"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="0.5"
        app:layout_constraintStart_toEndOf="@+id/txt_chargeF_getPhoneNumber"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_chargeF_getPhoneNumber"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_top_for_layout_right_top_in_chargeF"
        android:fontFamily="@font/iran_sans_mobile"
        android:gravity="center"
        android:text="09365874589"
        android:textColor="@color/black"
        android:textSize="@dimen/_5sdp"
        app:layout_constraintEnd_toStartOf="@+id/txt_chargeF_phoneNumber"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="0.5"
        app:layout_constraintStart_toEndOf="@+id/txt_chargeF_national_id"
        app:layout_constraintTop_toTopOf="parent" />

    <RelativeLayout
        android:id="@+id/button"
        android:layout_width="0dp"
        android:layout_height="@dimen/_13sdp"
        android:layout_marginEnd="@dimen/_5sdp"
        android:layout_marginStart="@dimen/_4sdp"
        android:background="@drawable/bg_border_white"
        app:layout_constraintBottom_toBottomOf="@+id/txt_chargeF_get_national_id"
        app:layout_constraintEnd_toStartOf="@+id/txt_chargeF_get_national_id"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txt_chargeF_get_national_id">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:fontFamily="@font/iran_sans_mobile"
            android:text="ویرایش"
            android:textSize="@dimen/_4sdp" />

        <ImageView
            android:layout_width="@dimen/_5sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/_4sdp"
            app:srcCompat="@drawable/ic_edit" />
    </RelativeLayout>


    <TextView
        android:id="@+id/txt_chargeF_total_inventory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_top_for_layout_right_top_in_chargeF"
        android:layout_marginEnd="10dp"
        android:fontFamily="@font/iran_sans_mobile"
        android:text="موجودی کل : "
        android:textSize="@dimen/_5sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="@dimen/_10sdp"
        app:layout_constraintEnd_toEndOf="@+id/txt_chargF_Name"
        app:layout_constraintTop_toBottomOf="@+id/txt_chargF_Name"
        app:layout_constraintVertical_bias="0.0" />


    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/iran_sans_mobile"
        android:layout_marginEnd="@dimen/_5sdp"
        android:textSize="@dimen/_5sdp"
        android:text="@string/txt_rail"
        app:layout_constraintBottom_toBottomOf="@+id/textView13"
        app:layout_constraintEnd_toStartOf="@+id/textView13"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/txt_chargeF_age"
        app:layout_constraintTop_toTopOf="@+id/textView13"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:fontFamily="@font/iran_sans_mobile"
        android:text="@string/txt_sample_cost"
        android:textSize="@dimen/_5sdp"
        android:textColor="@color/dark_lime_green"
        app:layout_constraintBottom_toBottomOf="@+id/txt_chargeF_total_inventory"
        app:layout_constraintEnd_toStartOf="@+id/txt_chargeF_total_inventory"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintTop_toTopOf="@+id/txt_chargeF_total_inventory"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txt_chargeF_return_file_inventory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/iran_sans_mobile"
        android:text="موجودی قابل استرداد"
        android:textSize="@dimen/_5sdp"
        app:layout_constraintBottom_toBottomOf="@+id/textView14"
        app:layout_constraintEnd_toEndOf="@+id/txt_chargeF_phoneNumber"
        app:layout_constraintTop_toTopOf="@+id/textView14"
        tools:layout_editor_absoluteX="547dp" />

        <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_15sdp"
        android:fontFamily="@font/iran_sans_mobile"
        android:text="@string/txt_sample_cost"
        android:textColor="@color/red_text"
        app:layout_constraintBottom_toBottomOf="@+id/txt_chargeF_return_file_inventory"
        app:layout_constraintEnd_toStartOf="@+id/txt_chargeF_return_file_inventory"
        app:layout_constraintTop_toTopOf="@+id/txt_chargeF_return_file_inventory" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/iran_sans_mobile"
        android:text="@string/txt_rail"
        android:textSize="@dimen/_5sdp"
        android:layout_marginEnd="@dimen/_5sdp"
        app:layout_constraintBottom_toBottomOf="@+id/textView16"
        app:layout_constraintEnd_toStartOf="@+id/textView16"
        app:layout_constraintTop_toTopOf="@+id/textView16" />

        <RelativeLayout
        android:id="@+id/relativeLayout55"
        android:layout_width="0dp"
        android:layout_height="@dimen/_13sdp"
        android:background="@drawable/bg_red_border"
        app:layout_constraintBottom_toBottomOf="@+id/textView16"
        app:layout_constraintEnd_toEndOf="@+id/button"
        app:layout_constraintStart_toStartOf="@+id/button"
        app:layout_constraintTop_toTopOf="@+id/textView16">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:fontFamily="@font/iran_sans_mobile"
            android:text="استرداد"
            android:textSize="@dimen/_4sdp" />

        <ImageView
            android:layout_width="@dimen/_5sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/_4sdp"
            app:srcCompat="@drawable/ic_edit" />
    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>